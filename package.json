{
  "name": "snake-game-prototype",
  "version": "0.2.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start1": "ng serve",
    "build1": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "electron .",
    "build": "ng build --prod --base-href ./",
    "pack": "electron-builder"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.6",
    "@angular/compiler": "^21.0.6",
    "@angular/core": "^21.0.6",
    "@angular/forms": "^21.0.6",
    "@angular/platform-browser": "^21.0.6",
    "@angular/router": "^21.0.6",
    "asar": "^3.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.6",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "typescript": "~5.9.2"
  },
  "build": {
    "npmRebuild": false,
    "appId": "com.example.myapp",
    "productName": "Snake Game Prototype",
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "files": [
      "dist/snake-game/browser/**/*",
      "main.js",
      "preload.js"
    ]
  }
}
